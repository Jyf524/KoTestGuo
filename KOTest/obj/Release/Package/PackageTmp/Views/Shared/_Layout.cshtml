<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width" />
    <title>@ViewBag.Title</title>
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")

    <link href="~/Content/bootstrap.css" rel="stylesheet" />
    <link href="~/Content/bootstrap.min.css" rel="stylesheet" />

    <script src="~/Scripts/knockout-3.5.1.debug.js"></script>
    <script src="~/Scripts/knockout-3.5.1.js"></script>
    <script src="~/Scripts/modernizr-2.6.2.js"></script>
   @* <script src="http://ajax.aspnetcdn.com/ajax/jquery/jquery-1.9.1.min.js"></script>*@
    @* <script src="~/Scripts/bootstrap.js"></script>*@
     <script src="https://cdn.staticfile.org/jquery/2.1.1/jquery.min.js"></script>
    <script src="~/Scripts/bootstrap.min.js"></script>
    <link href="~/Css/Style.css" rel="stylesheet" />
    <link href="~/Css/webuploader.css" rel="stylesheet" />
    <link href="~/Css/ChoiceCourse.css" rel="stylesheet" />
    <script src="~/Scripts/webuploader.nolog.min.js"></script>
    <script src="~/Scripts/Upload.js"></script>
</head>
<body>
    <nav class="navbar navbar-fixed-top" style="margin-bottom: 0px;">
        <div class="container-fluid">
            <div class="row">


                <div class="bs-example bs-navbar-top-example" data-example-id="navbar-static-top">
                    <nav class="navbar navbar-default navbar-static-top">
                        <!-- We use the fluid option here to avoid overriding the fixed width of a normal container within the narrow content columns. -->
                        <div class="container-fluid">
                            <div class="navbar-header">
                                <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#bs-example-navbar-collapse-8" aria-expanded="false">
                                    <span class="sr-only">Toggle navigation</span>
                                    <span class="icon-bar"></span>
                                    <span class="icon-bar"></span>
                                    <span class="icon-bar"></span>
                                </button>
                                <a class="navbar-brand" href="#"><span style="margin-left: 0px;">学生服务平台</span></a>
                            </div>

                            <!-- Collect the nav links, forms, and other content for toggling -->
                            <div class="navbar-collapse collapse" id="bs-example-navbar-collapse-8" aria-expanded="false" style="height: 1px;">
                                <ul class="nav navbar-nav nav_hide">
                                    <li class="active"><a href="~/KoTest/Index"><span class="glyphicon glyphicon-book" aria-hidden="true"></span><span style="margin-left: 5px;">学生管理</span></a></li>
                                    <li><a href="~/KoTest/CourseMentIndex"><span class="glyphicon glyphicon-user" aria-hidden="true"></span><span style="margin-left: 5px;">课程管理</span></a></li>
                                    <li><a href="~/KoTest/ClassMentIndex"><span class="glyphicon glyphicon-bookmark" aria-hidden="true"></span><span style="margin-left: 5px;">班级管理</span></a></li>
                                    <li><a href="~/KoTest/SemesterMentIndex"><span class="glyphicon glyphicon-signal" aria-hidden="true"></span><span style="margin-left: 5px;">学期管理</span></a></li>
                                    <li><a href="~/KoTest/TeacherMentIndex"><span class="glyphicon glyphicon-signal" aria-hidden="true"></span><span style="margin-left: 5px;">教师管理</span></a></li>
                                     <li><a href="~/KoTest/ChoiceCourseMentIndex"><span class="glyphicon glyphicon-signal" aria-hidden="true"></span><span style="margin-left: 5px;">选课管理</span></a></li>
                                </ul>
                                <ul class="nav navbar-nav navbar-right" style="margin-right: 80px;">
                                    <li style="cursor: pointer;"><a href="javascript:void(0)" id="UserNames"></a></li>
                                    <li style="cursor: pointer;"><a href="~/KoTest/Login" id="Quit">注销</a></li>
                                </ul>
                            </div>
                            <!-- /.navbar-collapse -->
                        </div>
                    </nav>
                </div>


            </div>
        </div>
    </nav>
    <div class="container-fluid">
        <div class="row">
            <div class="concent">
                @RenderSection("featured", required: false)
                <section class="content-wrapper main-content clear-fix" style="padding-top: 56px;">
                    @RenderBody()
                </section>

            </div>
        </div>
    </div>
</body>
</html>
